[application]

config/name="RainyBot-Core"
run/main_scene="res://Gui/Main.tscn"
run/low_processor_mode=true
boot_splash/image="res://logo.png"
boot_splash/fullsize=false
boot_splash/use_filter=false
config/icon="res://logo.png"
config/windows_native_icon="res://logo.ico"

[autoload]

InterfaceManager="*res://Core/Classes/Interface.gd"
DataManager="*res://Adapters/Mirai/MiraiCaster.gd"
ConfigManager="*res://Adapters/Mirai/MiraiAdapterConfig.gd"
MiraiManager="*res://Adapters/Mirai/MiraiManager.gd"
HttpRequestManager="*res://Core/HttpRequestManager.gd"
CommandManager="*res://Core/CommandManager.gd"
PluginManager="*res://Core/PluginManager.gd"

[display]

window/size/width=500
window/size/height=50

[logging]

file_logging/enable_file_logging=true

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/driver/driver_name="GLES2"
environment/default_clear_color=Color( 0.12549, 0.141176, 0.192157, 1 )
